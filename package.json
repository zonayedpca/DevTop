{
  "name": "devtop",
  "version": "0.2.1",
  "description": "DevTop - Essential Tools for Developers",
  "main": "app.js",
  "scripts": {
    "test-client": "npm run test --prefix client -- --watchAll=false",
    "test": "npm run test-client",
    "client": "npm start --prefix client",
    "build": "npm run build --prefix client",
    "electron": "electron .",
    "start": "concurrently \"npm run client\" \"npm run electron\"",
    "v:major": "npm version major",
    "v:minor": "npm version minor",
    "v:patch": "npm version patch",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist-win32": "npm run build && electron-builder build --win --ia3",
    "dist-win64": "npm run build && electron-builder build --win --x64",
    "dist-linux": "npm run build && electron-builder build --linux"
  },
  "build": {
    "appId": "devtop.essential",
    "mac": {
      "category": "devtop.essential"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zonayedpca/DevTop.git"
  },
  "keywords": [
    "desktop",
    "dev-tools",
    "developer-tools",
    "essential-dev",
    "essential-developer"
  ],
  "author": "Zonayed Ahmed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zonayedpca/DevTop/issues"
  },
  "homepage": "https://github.com/zonayedpca/DevTop#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.40.2"
  }
}
